{
  "name": "htmlgoddess",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "start": "ntl",
    "print": "webpack",
    "autoprint": "onchange 'src/**' -- webpack",
    "save": "git commit -am 'saving work'",
    "publish": "git push origin",
    "serve": "serve docs/",
    "format": "prettier src/ --write",
    "autoformat": "onchange 'src/**/*.html'  'src/**/*.css' -- prettier {{changed}} --write",
    "dev": "webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonascript/htmlgoddess.git"
  },
  "keywords": [
    "html"
  ],
  "author": "Jonathan Crockett",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jonascript/htmlgoddess/issues"
  },
  "htmlgoddess": {
    "output": "/docs"
  },
  "homepage": "https://github.com/jonascript/htmlgoddess#readme",
  "devDependencies": {
    "@webpack-cli/serve": "0.2.0",
    "clean-webpack-plugin": "3.0.0",
    "command-line-args": "5.1.1",
    "copy-webpack-plugin": "6.0.1",
    "css-loader": "3.5.3",
    "glob": "7.1.6",
    "html-beautify-webpack-plugin": "1.0.5",
    "html-loader": "1.1.0",
    "html-replace-webpack-plugin": "2.5.6",
    "html-webpack-plugin": "4.3.0",
    "mini-css-extract-plugin": "0.9.0",
    "ntl": "5.0.0",
    "onchange": "7.0.2",
    "prettier": "2.0.5",
    "pretty": "2.0.0",
    "style-loader": "1.2.1",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.11.0",
    "webpack-fix-style-only-entries": "0.5.0"
  },
  "dependencies": {
    "postcss-loader": "3.0.0",
    "serve": "11.3.1"
  }
}